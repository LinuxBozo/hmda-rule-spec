[
    {
        "id": "Q033",
        "description": "If respondent is a bank, savings association, or independent mortgage company, and if any parent company exists, then parent name, address, city, state and zip code should not = blank.",
        "explanation": "Parent name, address, city, state, or zip code is missing.",
        "rule": {
            "if": {
                "property": "legalEntityID",
                "condition": "call",
                "function": "isChildFI",
                "args": ["legalEntityID"]
            },
            "then": {
                "and": [
                    {
                        "property": "parentName",
                        "condition": "not_empty"
                    },
                    {
                        "property": "parentAddress",
                        "condition": "not_empty"
                    },
                    {
                        "property": "parentCity",
                        "condition": "not_empty"
                    },
                    {
                        "property": "parentState",
                        "condition": "not_empty"
                    },
                    {
                        "property": "parentZip",
                        "condition": "not_empty"
                    }
                ]
            }
        }
    },
    {
        "id": "Q020",
        "description": "Institution address should not = parent address.",
        "explanation": "Institution address equals parent address.",
        "rule": {
            "if": {
                "property": "parentAddress",
                "condition": "not_empty"
            },
            "then": {
                "property": "respondentAddress",
                "condition": "not_equal_property",
                "value": "parentAddress"
            }
        }
    },
    {
        "id": "Q012",
        "description": "The reported Tax ID number on the transmittal sheet of your HMDA data file does not match the Tax ID number reported in the previous calendar year submission.",
        "explanation": "Tax ID number on TS does not match the Tax ID number reported in the previous calendar year submission.",
        "rule": {
            "property": "taxID",
            "condition": "call",
            "function": "isTaxIDTheSameAsLastYear",
            "args": ["legalEntityID", "taxID"]
        }
    },
    {
        "id": "NPRM070",
        "description": "If Ethnicity of Borrower or Applicant = 4 and Sex of Borrower or Applicant = 4 and Race of Borrower or Applicant 1 = 7 and Age of Borrower or Applicant = 1 or if Ethnicity of Co-Borrower or Co-Applicant = 5 and Sex of Co-Borrower or Co-Applicant = 5 and Race of Co-Borrower or Co-Applicant 1 = 8 and Age of Co-Borrower or Co-Applicant = 2 and Applicant Income = NA and Action Taken is not equal to 6, Rate Spread should = NA.",
        "explanation": "If the applicant or co-applicant is not a natural person, rate spread should = NA.",
        "rule": {
            "if": {
                "or": [
                    {
                        "and": [
                            {
                                "property": "applicantEthnicity",
                                "condition": "equal",
                                "value": "4"
                            },
                            {
                                "property": "applicantSex",
                                "condition": "equal",
                                "value": "4"
                            },
                            {
                                "property": "applicantRace1",
                                "condition": "equal",
                                "value": "7"
                            },
                            {
                                "property": "applicantAge",
                                "condition": "equal",
                                "value": "1"
                            },
                            {
                                "property": "applicantIncome",
                                "condition": "equal",
                                "value": "NA"
                            }
                        ]
                    },
                    {
                        "and": [
                            {
                                "property": "coapplicantEthnicity",
                                "condition": "equal",
                                "value": "5"
                            },
                            {
                                "property": "coapplicantSex",
                                "condition": "equal",
                                "value": "5"
                            },
                            {
                                "property": "coapplicantRace1",
                                "condition": "equal",
                                "value": "8"
                            },
                            {
                                "property": "coapplicantAge",
                                "condition": "equal",
                                "value": "2"
                            },
                            {
                                "property": "applicantIncome",
                                "condition": "equal",
                                "value": "NA"
                            }
                        ]
                    }
                ]
            },
            "then": {
                "property": "rateSpread",
                "condition": "equal",
                "value": "NA"
            }
        }
    }
]
